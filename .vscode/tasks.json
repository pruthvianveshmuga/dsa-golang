  "version": "2.0.0",
  "type": "shell",
  "command": "go",
  "cwd": "${workspaceFolder}",
  "tasks": [
    {
      "label": "test",
      "args": ["test", "./..."],
      "group": "test"
    },
    {
      "label": "test verbose",
      "args": ["test", "./...", "-v"],
      "group": "test"
    }
  ]
}
